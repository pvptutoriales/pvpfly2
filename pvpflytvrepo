<addon id="plugin.video.p2p-streams"
       name="p2p-streams"
       version="0.2.7"
       provider-name="enen92 - fightnight">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.0.8"/>
